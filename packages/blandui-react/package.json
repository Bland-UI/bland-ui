{
	"name": "blandui-react",
	"version": "0.0.1",
	"description": "",
	"main": "dist/cjs/index.cjs",
	"module": "dist/esm/index.mjs",
	"types": "dist/types/index.d.ts",
	"source": "src/index.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.cjs",
			"types": "./dist/types/index.d.ts"
		},
		"./components/*": {
			"import": "./dist/esm/components/*/index.mjs",
			"require": "./dist/cjs/components/*/index.cjs",
			"types": "./dist/types/components/*/index.d.ts"
		},
		"./tailwind": {
			"import": "./dist/esm/tailwind.mjs",
			"require": "./dist/cjs/tailwind.cjs",
			"types": "./dist/types/tailwind.d.ts"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "rollup -cw",
		"build": "rollup -c",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"peerDependencies": {
		"react": ">=18",
		"react-dom": ">=18",
		"tailwindcss": "^3"
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "^11.1.6",
		"@storybook/react": "^8.1.1",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"blandui": "workspace:*",
		"esbuild": "^0.21.3",
		"eslint": "8.55.0",
		"eslint-config-lludol": "^0.2.1",
		"eslint-config-next": "^14.2.3",
		"glob": "^10.4.1",
		"rimraf": "^5.0.7",
		"rollup": "^4.18.0",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-use-client": "^1.4.0",
		"typescript": "^5.4.5"
	},
	"author": "lludol",
	"license": "MIT",
	"dependencies": {
		"blandui": "workspace:*",
		"clsx": "^2.1.1",
		"tailwind-merge": "^2.3.0"
	}
}
