{
	"plugins": [
		"@typescript-eslint"
	],
	"extends": [
		"next/core-web-vitals",
		"plugin:@typescript-eslint/recommended",
		"eslint-config-lludol"
	],
	"rules": {
		"react-hooks/exhaustive-deps": "error",
		"require-jsdoc": "off",
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": "error",
		"max-len": "off",
		"@typescript-eslint/no-explicit-any": "warn"
	}
}
